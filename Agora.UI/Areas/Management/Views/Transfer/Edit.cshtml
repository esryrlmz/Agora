@model Transfer




<div class="container-fluid">
	<div class="row">
       <div class="col-lg-8">
          <div class="panel panel-default">
             <div class="panel-heading">
               <span  style="color: #31708f"><b> TRANSFER DUZENLEME EKRANI</b></span>
             </div>
             <div class="panel-body">

                <div class="row">
                    <div class="col-lg-12">
                        <form role="form">
							<div class="form-group">
                                 <div class="row">
                               <div class="col-lg-4"> <label> 
                                   <b> Transfer İl:</b>  </label></div>
                               <div class="col-lg-8"> <select class="form-control select2">
                                  <option label="İl Seçiniz"></option>
                                </select>
                                </div>
                                </div>
                             </div>
                             	<div class="form-group">
                                 <div class="row">
                               <div class="col-lg-4"> <label> 
                                   <b> Transfer İlce:</b>  </label></div>
                               <div class="col-lg-8"> <select class="form-control select2">
                                  <option label="İlçe Seçiniz"></option>
                                </select>
                                </div>
                                </div>
                             </div>
                            <div class="form-group">
                                <div class="row">
                                <div class="col-lg-4"> <label>  <b> Adres</b></label></div>
                                 <div class="col-lg-8"><textarea class="form-control" > adres bilgisi buraya gelecek admin düzenleyebilir</textarea> </div></div></div>

                            <div class="form-group">
                                 <div class="row">
                               <div class="col-lg-4"> <label>  <b>Transfer Durumu</b></label></div>
                                 <div class="col-lg-8">
                                     <select class="form-control select2">
                                    <option label="Kargo ile Transfer edildi"></option>
                                    <option label="Elden Transfer edildi"></option>
                                    <option label="Rezerve edildi"></option>
                                     </select>
                                 </div></div>
                            </div>
                           
                             <div class="form-group">
                               <button type="submit" class="btn" style="background-color: #31708f; float: right; color:#ffffff">GÜNCELLE</button>
                             </div>
                           </form>                    
						
                    </div>
										
					
                    <!-- /.col-lg-6 (nested) -->
                                        
                </div>

             </div>
          </div>
        </div>
      </div>

</div>